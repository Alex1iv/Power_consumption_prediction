{
    "//": "Set seed value",
    "random_seed" : 42,

    "//": "Model folder",
    "path_models" : "../models/",
    "data_dir" : "../data/",
    "path_figures": "../figures/",

    "//": "Функция Callbacks",
    "reduce_patience":15,
    "stop_patience":200,
    "monitor":"mse",
    "verbose":1,
    "mode":"max",
    "save_best_only": true,
    "restore_best_weights": true,
    "factor": 0.9,
    "min_lr_coeff":0.0001,

    "//": "model",
    "model":{
        "learning_rate":0.001
    }


}